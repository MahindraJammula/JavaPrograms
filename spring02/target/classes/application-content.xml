<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="laptop1" class="org.kodnest.spring02.Laptop">
		<property name="serialNumber" value="111" />
		<property name="manufacturer" value="HP" />
		<property name="cost" value="85000" />
	</bean>
	
	<bean id="laptop2" class="org.kodnest.spring02.Laptop">
	<constructor-arg name="serialNumber" value="222"/>
	<constructor-arg name="manufacturer" value="Dell"/>
	<constructor-arg name="cost" value="450000"/>
	</bean>

</beans>

